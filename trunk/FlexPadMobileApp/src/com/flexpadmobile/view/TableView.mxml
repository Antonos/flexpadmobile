<?xml version="1.0" encoding="utf-8"?>
<view:FlexPadMobileView xmlns:fx="http://ns.adobe.com/mxml/2009"
						xmlns:s="library://ns.adobe.com/flex/spark"
						xmlns:flexpadmobile="http://code.google.com/p/flexpadmobile/"
						xmlns:path="com.flexpadmobile.path.*"
						xmlns:view="com.flexpadmobile.view.*"
						title="Table">
	
	<s:BorderContainer width="440" backgroundColor="0xdfe2e7" borderVisible="false">
		<flexpadmobile:Table left="20" top="20" bottom="20" width="400">
			<flexpadmobile:TableRow>
				<s:HGroup width="100%">
					<s:Label styleName="listItemRendererLabel" text="Label One"/>
					<s:Spacer width="100%"/>
					<flexpadmobile:Switch/>
				</s:HGroup>
			</flexpadmobile:TableRow>
			<flexpadmobile:TableRow>
				<s:HGroup width="100%">
					<s:Label styleName="listItemRendererLabel" text="Label Two"/>
					<s:Spacer width="100%"/>
					<s:HGroup verticalAlign="middle">
						<flexpadmobile:ListItemRendererHintLabel text="Two"/>
						<path:ArrowPath>
							<path:fill>
								<s:SolidColor color="0x919191"/>
							</path:fill>
						</path:ArrowPath>
					</s:HGroup>
				</s:HGroup>
			</flexpadmobile:TableRow>
			<flexpadmobile:TableRow>
				<s:HGroup width="100%">
					<s:Label styleName="listItemRendererLabel" text="Label Three"/>
					<s:Spacer width="100%"/>
					<flexpadmobile:ArrowButton/>
				</s:HGroup>
			</flexpadmobile:TableRow>
		</flexpadmobile:Table>
	</s:BorderContainer>
</view:FlexPadMobileView>
